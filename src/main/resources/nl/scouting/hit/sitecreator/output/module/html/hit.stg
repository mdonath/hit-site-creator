hit(hit) ::= "
<html>
<head><title>HIT</title></head>
<body id=\"test\">
<h1>HIT $hit.jaar$</h1>
$hitPlaatsen(hit)$
</body>
</html>"

hitPlaatsen(hit) ::= "
<ul>
	$hit.hitPlaatsen:hitPlaats()$
</ul>
"

hitPlaats(plaats) ::= "<li>$plaats.naam$$hitKampen(plaats)$</li>
"

hitKampen(plaats) ::= "
<ul>
	$plaats.hitKampen:hitKamp()$
</ul>
"

hitKamp(kamp) ::= "<li>$kamp.naam$</li>
"